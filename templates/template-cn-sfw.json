{
  "log": {
    "level": "fatal",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "remote",
        "address": "https://1.0.0.1/dns-query"
      },
      {
        "tag": "direct",
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "direct"
      }
    ],
    "independent_cache": true,
    "reverse_mapping": true,
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    }
  },
  "ntp": {
    "enabled": true,
    "server": "pool.ntp.org",
    "detour": "direct"
  },
  "inbounds": [
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7890,
      "sniff": true
    },
    {
      "type": "tun",
      "inet4_address": "172.19.0.1/30",
      "inet6_address": "fdfe:dcba:9876::1/126",
      "auto_route": true,
      "strict_route": true,
      "stack": "gvisor",
      "sniff": true,
      "exclude_package": [
        "com.android.captiveportallogin"
      ],
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 7890
        }
      }
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "default",
      "interrupt_exist_connections": true,
      "outbounds": [
        "auto",
        "manual",
        "direct",
        "block"
      ]
    },
    {
      "type": "selector",
      "tag": "manual",
      "interrupt_exist_connections": true,
      "outbounds": []
    },
    {
      "type": "urltest",
      "tag": "auto",
      "url": "https://cp.cloudflare.com",
      "interval": "3h",
      "outbounds": []
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "geoip": {
      "download_url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@release/geoip.db",
      "download_detour": "direct",
      "path": "geoip.db"
    },
    "geosite": {
      "download_url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@release/geosite.db",
      "download_detour": "direct",
      "path": "geosite.db"
    },
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "outbound": "dns"
      },
      {
        "geosite": [
          "category-ads-all",
          "malware",
          "phishing",
          "cryptominers",
          "category-porn",
          "nsfw"
        ],
        "outbound": "block"
      },
      {
        "geoip": [
          "malware",
          "phishing"
        ],
        "outbound": "block"
      },
      {
        "geosite": [
          "private",
          "cn"
        ],
        "outbound": "direct"
      },
      {
        "geoip": [
          "private",
          "cn"
        ],
        "outbound": "direct"
      },
      {
        "geosite": "geolocation-!cn",
        "outbound": "default"
      }
    ]
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "xd",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/gh-pages.zip",
      "external_ui_download_detour": "direct",
      "store_fakeip": true
    },
    "debug": {
      "memory_limit": "200m",
      "oom_killer": true
    }
  }
}
